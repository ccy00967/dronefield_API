option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: core.wsgi:application

container_commands:
  01_migrate:
    command: "python manage.py migrate"
    leader_only: true

  02_permissions:
    command: "chmod +x manage.py"
  
  03_collectstatic:
    command: "python manage.py collectstatic --noinput"
    leader_only: true
